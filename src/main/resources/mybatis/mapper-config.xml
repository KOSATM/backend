<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "https://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
  <settings>
    <setting name="mapUnderscoreToCamelCase" value="true" />
    <setting name="jdbcTypeForNull" value="NULL" />
  </settings>

  <typeAliases>
    <package name="com.example.demo.travelgram.review.dto.entity"/>
    <package name="com.example.demo.planner.plan.dto.entity"/>
    <package name="com.example.demo.common.chat.dto"/>

    <package name="com.example.demo.travelgram.review.ai.dto.entity"/>
    <package name="com.example.demo.supporter.checklist.dto.entity"/>
    <package name="com.example.demo.supporter.imageSearch.dto.entity"/>
    <package name="com.example.demo.supporter.imageSearch.dto.response"/>
    <package name="com.example.demo.supporter.map.dto.entity"/>
  </typeAliases>

  <typeHandlers>
    <typeHandler handler="com.example.demo.common.global.typehandler.VectorTypeHandler" />
    <typeHandler handler="com.example.demo.common.global.typehandler.JsonListTypeHandler" />
  </typeHandlers>

</configuration>
