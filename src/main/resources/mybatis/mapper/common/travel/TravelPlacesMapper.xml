<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.common.travel.dao.TravelPlaceDao">
    <select id="selectImgUrlByTitle" resultType="string" parameterType="string">
        SELECT first_image
        FROM travel_places
        WHERE title = #{title}
    </select>
</mapper>
